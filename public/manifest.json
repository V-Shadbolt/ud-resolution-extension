{
   "name": "Unstoppable Domains Extension",
   "description": "Send Crypto to Unstoppable domains",
   "version": "1.0",
   "manifest_version": 3,
   "icons": {
       "16": "ud-logo.png",
       "48": "ud-logo.png",
       "128": "ud-logo.png"
   },
  "action": {
    "default_popup": "index.html",
    "default_title": "Unstoppable Domains Extension"
  },
  "permissions": [
    "webRequest",
    "storage",
    "scripting"
],
"host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
]
}